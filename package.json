{
  "name": "assetatlas",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:css && tsc -p tsconfig.build.json && vite build",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec \"node --loader ts-node/esm src/index.ts\" & vite dev & tsc -w",
    "build:css": "npx tailwindcss -i ./src/svelteStyles/input.css -o ./src/svelteStyles/output.css",
    "test": "vitest run --exclude=tests/performance.test.ts --reporter verbose --silent",
    "test:perf": "vitest run tests/performance.test.ts --reporter verbose --silent",
    "coverage": "vitest run --exclude=tests/performance.test.ts --coverage --coverage.include=src/** --reporter dot --silent",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" --fix",
    "check": "tsc --noEmit",
    "migrate_down": "migrate-mongo down -f migrate-mongo-config.cjs",
    "migrate_up": "migrate-mongo up -f migrate-mongo-config.cjs",
    "migrate_status": "migrate-mongo status -f migrate-mongo-config.cjs",
    "migrate_create": "migrate-mongo create -f migrate-mongo-config.cjs"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-typescript": "^7.25.7",
    "@eslint/js": "^9.13.0",
    "@skeletonlabs/skeleton": "^2.10.3",
    "@skeletonlabs/tw-plugin": "^0.4.0",
    "@sveltejs/vite-plugin-svelte": "^2.5.3",
    "@tsconfig/svelte": "^5.0.4",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.8.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.14.0",
    "globals": "^15.11.0",
    "mongodb-memory-server": "^10.1.2",
    "postcss": "^8.4.47",
    "supertest": "^7.0.0",
    "svelte-preprocess": "^6.0.2",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^4.0.0",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "express": "^4.21.0",
    "fuse.js": "^7.0.0",
    "gridfs-stream": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "migrate-mongo": "^12.1.3",
    "mongodb": "^5.9.1",
    "mongoose": "^8.9.5",
    "multer": "^1.4.4",
    "multer-gridfs-storage": "^5.0.2",
    "nodemon": "^3.1.7",
    "svelte": "4.2.19",
    "svelte-icons": "^2.1.0",
    "svelte-routing": "^2.13.0",
    "tslib": "^2.7.0"
  }
}
